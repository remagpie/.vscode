			{"key": "shift+v", "command": "moderator.setModifier", "when": "moderator.mode == 'navigate'", "args": "v"},
			{"key": "ctrl+v", "mac": "cmd+v", "command": "moderator.setModifier", "args": "v"},
			{"key": "ctrl+shift+v", "mac": "cmd+shift+v", "command": "moderator.setModifier", "args": "v"},
