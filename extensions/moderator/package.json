{
	"name": "moderator",
	"displayName": "moderator",
	"description": "Remagpie keymap",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": ["*"],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "moderator.resetState",
				"title": "Moderator: Reset state"
			},
			{
				"command": "moderator.switchMode",
				"title": "Moderator: Switch to Navigate Mode",
				"args": "navigate"
			},
			{
				"command": "moderator.switchMode",
				"title": "Moderator: Switch to Edit Mode",
				"args": "edit"
			}
		],
