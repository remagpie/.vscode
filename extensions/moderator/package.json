			{"key": "space", "command": "moderator.switchMode", "when": "moderator.mode == 'navigate'", "args": "edit"},
			{"key": "ctrl+space", "mac": "cmd+space", "command": "moderator.switchMode", "when": "moderator.mode == 'edit'", "args": "navigate"},
