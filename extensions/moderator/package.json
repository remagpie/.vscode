			{"key": "s", "command": "moderator.cursorLeft", "when": "moderator.mode == 'navigate'"},
			{"key": "ctrl+s", "mac": "cmd+s", "command": "moderator.cursorLeft"},
			{"key": "shift+s", "command": "moderator.cursorLeftSelect", "when": "moderator.mode == 'navigate'"},
